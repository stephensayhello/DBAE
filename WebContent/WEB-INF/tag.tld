<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>2.0</jsp-version>
	<short-name>Partial Tags</short-name>
	
	<tag>
		<name>bootstrap</name>
		<tag-class>de.tags.Bootstrap</tag-class>
	</tag>
	<tag>
		<name>profilnav</name>
		<tag-class>de.tags.Profilnav</tag-class>
	</tag>
	<tag>
		<name>modal</name>
		<tag-class>de.tags.Modal</tag-class>
		<attribute>
			<name>messages</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
		<tag>
		<name>artikel</name>
		<tag-class>de.tags.ArtikelView</tag-class>
		<attribute>
			<name>produkt</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>	
			<name>bewertungen</name>
			<required>false</required>
		</attribute>
	</tag>
	
	<tag>
		<name>tagesangebot</name>
	<tag-class>de.tags.TagesAngebot</tag-class>
	<attribute>
		<name>produkt</name>
		<required>true</required>
		<rtexprvalue>true</rtexprvalue>
	</attribute>
	</tag>
	
	<tag>
		<name>navKunde</name>
		<tag-class>de.tags.NavKunde</tag-class>
		<attribute>
		<name>rolle</name>
		<required>true</required>
		<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<tag>
		<name>footer</name>
		<tag-class>de.tags.Footer</tag-class>
	</tag>
</taglib>
